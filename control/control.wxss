.touch-control {
	width: 480rpx;
	border-radius: 60rpx;
	border: 20rpx solid #E4E4E4;
	background-color: #E4E4E4;
	height: 120rpx;
	overflow: hidden;
	font-weight: bold;
	font-size: 40rpx;
	color: #00000063;
}

.touch-control-box>image {
	height: 36rpx;
	width: 74rpx;
}

.touch-control-icon {
	height: 80rpx;
	border-radius: 40rpx;
	width: 80rpx;
	padding: 16rpx;
	background: #282828;
}

.touch-control-icon>image {
	width: 48rpx;
	height: 48rpx;
}

.group_2 {
	gap: 24rpx;
}

.group_2>view {
	flex: 1;
	height: 110rpx;
	background: #fff;
	border-radius: 30rpx;
}

.group_3 {
	gap: 24rpx;
	font-weight: bold;
	margin-top: 30rpx;
}

.group_3>view {
	height: 186rpx;
	background: #fff;
	border-radius: 30rpx;
}

.group_3>view:nth-child(1) {
	flex: 1;
	font-size: 26rpx;
	padding: 0 30rpx;
}

.group_3>view:nth-child(2) {
	width: 216rpx;
	font-size: 30rpx;
	overflow: hidden;
}

.group_3>view:nth-child(2)>view {
	flex: 1;
}

.group_3>view:nth-child(2)>view:nth-child(2) {
	font-size: 48rpx;
	color: #fff;
	width: 100%;
	background: linear-gradient(180deg, #60B7FF 0%, #008CFF 100%);
}